{
    "zone_key": "zone-default-zone",
    "listener_key": "egress-listener-b0",
    "domain_keys": [
        "egress-domain-b0"
    ],
    "name": "egress-b0",
    "ip": "127.0.0.1",
    "port": 9092,
    "protocol": "http_auto",
    "active_network_filters": ["gm.observables_tcp", "envoy.tcp_proxy"],
    "network_filters": {
        "gm_observables_tcp": {
            "useKafka": true,
            "kafkaServerConnection": "kafka-broker-0.kafka.svc.cluster.local:9092",
            "topic": "network-obs-topic",
            "eventTopic": "network-obs-topic",
            "logLevel": "debug",
            "decodeToProtocol": "kafka"
        },
        "envoy_tcp_proxy": {
            "stat_prefix": "broker0_ingress_tcp",
            "cluster": "broker0"
        }
    }
}