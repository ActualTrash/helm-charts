{
    "zone_key": "zone-default-zone",
    "listener_key": "egress-listener-b1",
    "domain_keys": [
        "egress-domain-b1"
    ],
    "name": "egress-b1",
    "ip": "127.0.0.1",
    "port": 9093,
    "protocol": "http_auto",
    "active_network_filters": ["gm.observables_tcp", "envoy.tcp_proxy"],
    "network_filters": {
        "gm_observables_tcp": {
            "useKafka": true,
            "kafkaServerConnection": "kafka-broker-1.kafka.svc.cluster.local:9093",
            "topic": "network-obs-topic",
            "eventTopic": "network-obs-topic"
        },
        "envoy_tcp_proxy": {
            "stat_prefix": "broker1_ingress_tcp",
            "cluster": "broker1"
        }
    }
}